<div id="map" style="width: 850px; height: 420px">
</div>

<div class="results js-results" style="width: 850px"></div>

<script type="text/ecmascript">
  ;(function (w, $) {
    if (w.locator.Map) {
      let map = new w.locator.Map({
        lat: <%= Moscow::COORDS[:lat] %>,
        long: <%= Moscow::COORDS[:long] %>
      }, "<%= home_near_houses_path %>")

      ymaps.ready(map.addMap.bind(map));
    } else {
      throw "Locator Map class not found!"
    }
  })(window, jQuery);

</script>
